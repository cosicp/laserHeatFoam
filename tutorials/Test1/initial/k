{
    leftWall
    {
      type		zeroGradient;
    }
    rightWall
    {
      type		zeroGradient;
    }
    topWall
    {
    type		zeroGradient;
      }
    bottomWall
    {
	type		zeroGradient;
    }
    front
    {
	type		zeroGradient;
    }
    back
    {
      type		zeroGradient;
    }
    solidInterface
    {
      type		zeroGradient;
    }
}

